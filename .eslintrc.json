{
  "extends": "airbnb-base",

  "parser": "babel-eslint",

  "plugins": [
    "babel",
    "import"
  ],

  "rules": {
    "array-callback-return": 0,
    "class-methods-use-this": 0,
    "consistent-return": 0,
    "curly": 0,
    "func-names": 0,
    "no-console": ["error", { "allow": ["log", "warn", "error"] }],
    "no-param-reassign": 0,
    "no-shadow": 0,
    "no-plusplus": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": 0,
    "no-useless-escape": 0,
    "radix": 0
  }
}
